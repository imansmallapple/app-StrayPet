{
  "module": {
    "name": "mobile",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "MobileAbility",
    "deviceTypes": [
      "phone",
      "wearable"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "MobileAbility",
        "srcEntry": "./ets/mobileability/MobileAbility.ets",
        "description": "$string:MobileAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:MobileAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "name": "MobileBackupAbility",
        "srcEntry": "./ets/mobilebackupability/MobileBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      }
    ]
  }
}